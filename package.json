  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chitacan/adb-bridge/issues"
  },
  "homepage": "https://github.com/chitacan/adb-bridge",
  "dependencies": {
    "body-parser": "^1.6.5",
    "cookie-parser": "^1.3.2",
    "debug": "^2.0.0",
    "express": "^4.8.5",
    "jade": "^1.5.0",
    "lodash": "^2.4.1",
    "morgan": "^1.2.3",
    "socket.io": "^1.0.6",
    "underscore": "^1.6.0",
    "socket.io-client": "^1.1.0",
    "commander": "^2.3.0",
    "inquirer": "^0.7.1",
    "restler": "^3.2.2"
  },
  "devDependencies": {
    "longjohn": "^0.2.4",
    "mocha": "^1.21.4",
    "restler": "^3.2.2",
    "should": "^4.0.4",
    "supertest": "^0.13.0"
  }
