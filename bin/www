#!/usr/bin/env node
var debug = require('debug')('server:www')
  , www   = require('../app')({});

var app    = www.app
  , server = www.server;

app.set('port', process.env.PORT || 9500);

server.listen(app.get('port'), function() {
  debug('Express server listening on port ' + server.address().port);
});
